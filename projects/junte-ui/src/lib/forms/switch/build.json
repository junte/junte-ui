{
  "composition": {
    "section": "forms",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "switch/_switch.scss",
      "switch/switch-group/_switch-group.scss"
    ],
    "to": "_switch.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-switch-host",
      "html": {
        "from": "switch.component.html",
        "to": "switch.encapsulated.html"
      },
      "scss": {
        "from": "switch.component.scss",
        "to": "../../assets/styles/forms/switch/_switch.scss"
      }
    },
    {
      "host": "jnt-switch-group-host",
      "html": {
        "from": "switch-group/switch-group.component.html",
        "to": "switch-group/switch-group.encapsulated.html"
      },
      "scss": {
        "from": "switch-group/switch-group.component.scss",
        "to": "../../assets/styles/forms/switch/switch-group/_switch-group.scss"
      }
    }
  ]
}
