{
  "name": "@junte/ui",
  "version": "0.0.0",
  "description": "Quality Angular UI components kit",
  "keywords": [
    "angular",
    "components",
    "ui",
    "user interface",
    "ui kit",
    "material"
  ],
  "homepage": "https://junte-ui.com",
  "bugs": "https://gitlab.com/junte/junte-ui/-/issues",
  "license": "MIT",
  "author": {
    "name": "Anton Breslavsky",
    "email": "anton@junte.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/junte/junte-ui.git"
  },
  "scripts": {
    "sass": "sass",
    "ng": "ng",
    "gulp": "gulp",
    "cp": "cp",
    "clean": "rm -rf ../../dist/junte-ui",
    "styles": "gulp build --gulpfile ./src/scripts/styles.ts --cwd ./",
    "iconfont": "gulp build --gulpfile ./src/scripts/iconfont/iconfont.ts --cwd ./",
    "encapsulation": "gulp build --gulpfile ./node_modules/junte-encapsuler/encapsulation.js --cwd ./",
    "encapsulation:watch": "gulp build --gulpfile ./node_modules/junte-encapsuler/encapsulation.js --cwd ./ --watch",
    "assets:create": "rm -rf ../../dist/junte-ui/lib/assets && mkdir ../../dist/junte-ui/lib/assets/ && mkdir ../../dist/junte-ui/lib/assets/icons && mkdir ../../dist/junte-ui/lib/assets/styles",
    "assets:copy:fonts": "cp -r ./src/lib/assets/fonts ../../dist/junte-ui/lib/assets",
    "assets:copy:icons": "cp -r ./src/lib/assets/icons/animated ../../dist/junte-ui/lib/assets/icons/animated && cp -r ./src/lib/assets/icons/svg ../../dist/junte-ui/lib/assets/icons/svg",
    "assets:copy:styles": "cp ./src/lib/assets/styles/*.scss ../../dist/junte-ui/lib/assets/styles",
    "copy:assets": "npm run assets:create && npm run assets:copy:fonts && npm run assets:copy:icons && npm run assets:copy:styles",
    "copy:iconfont": "cd ./src/scripts/iconfont && tsc && cp -r iconfont.js template ../../../../../dist/junte-ui",
    "copy:version": "cp ./VERSION ../../dist/junte-ui",
    "copy:polyfills": "cp -r ./src/polyfills ../../dist/junte-ui",
    "copy:files": "npm run copy:assets && npm run copy:iconfont && npm run copy:version && npm run copy:polyfills",
    "build": "npm run clean && npm run encapsulation && npm run iconfont && ng build --prod && npm run copy:files && npm run styles && npm run version:inject",
    "copy": "wait-on -i 3000 ../../dist/junte-ui/bundles/junte-ui.umd.js && npm run copy:files && npm run styles:watch",
    "serve": "npm run clean && run-p --continue-on-error=false -l -n *:watch copy",
    "styles:watch": "wait-on ../../dist/junte-ui/template && gulp build --gulpfile ./src/scripts/styles.ts --cwd ./ --watch",
    "build:watch": "ng build junte-ui --watch",
    "pack": "cd ../../dist/junte-ui && npm pack",
    "publish:npm": "npm run pack && npm publish --verbose --tag ${tag} --access public ../../dist/junte-ui/junte-ui-$(cat VERSION).tgz",
    "publish": "tag=unstable npm run publish:npm",
    "publish:rc": "tag=next npm run publish:npm",
    "publish:prod": "tag=latest npm run publish:npm",
    "pack:version": "npm run build && npm run version:inject && npm run pack",
    "publish:version": "npm run build && npm run version:inject && npm run publish",
    "clean:npm": "npm cache clean --force && rm -rf -- node_modules package-lock.json",
    "refresh": "npm run clean:npm && npm install",
    "version:inject": "gulp inject --gulpfile ./src/scripts/version.ts --cwd ../../dist/junte-ui/"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular-devkit/build-ng-packagr": "^0.901.12",
    "@angular/animations": "^9.0.4",
    "@angular/cli": "^9.1.12",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^9.0.4",
    "@angular/language-service": "^9.0.4",
    "@angular/localize": "^9.0.4",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/router": "^9.0.4",
    "gulp": "4.0.0",
    "gulp-consolidate": "0.2.0",
    "gulp-debug": "4.0.0",
    "gulp-iconfont": "10.0.1",
    "gulp-rename": "1.4.0",
    "gulpclass": "0.2.0",
    "html-webpack-plugin": "^4.4.1",
    "junte-encapsuler": "0.0.46",
    "map-stream": "0.0.7",
    "minimist": "1.2.5",
    "ng-packagr": "9.0.2",
    "node-sass": "^4.14.1",
    "query-ast": "1.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6.5.4",
    "scss-parser": "1.0.4",
    "ts-node": "8.6.2",
    "tsickle": "0.37.1",
    "typescript": "3.7.2",
    "zone.js": "0.10.3"
  },
  "dependencies": {
    "@ngx-translate/core": "12.1.2",
    "date-fns": "2.10.0",
    "ngx-clipboard": "^14.0.1",
    "ngx-logger": "4.1.8"
  },
  "peerDependencies": {
    "@angular/animations": "^9.0.4",
    "@angular/common": "^9.0.4",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^9.0.4",
    "@angular/router": "^9.0.4",
    "rxjs": "^6.5.3"
  }
}
