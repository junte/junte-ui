{
  "name": "junte-ui-app",
  "version": "0.0.1",
  "description": "Junte UI test app",
  "scripts": {
    "sass": "sass",
    "ng": "ng",
    "gulp": "gulp",
    "postinstall": "cd ./projects/junte-ui && npm i",
    "clean": "rimraf ./dist/junte-ui-app",
    "iconfont": "gulp build --gulpfile ./projects/junte-ui/src/scripts/iconfont/iconfont.ts --cwd ./",
    "todos": "gulp build --gulpfile ./src/scripts/todos.ts --cwd ./",
    "serve:lib": "cd ./projects/junte-ui && npm run serve",
    "serve": "wait-on ./dist/junte-ui/lib/assets/styles/shared && ng serve --poll 5000",
    "start": "run-p --continue-on-error=false -l -n serve:lib serve",
    "extract-i18n": "ng xi18n --outputPath=locale && xliffmerge --profile xliffmerge.json",
    "build:lib": "cd ./projects/junte-ui && npm run build",
    "build": "npm run clean && npm run build:lib && npm run iconfont && ng build --prod",
    "clean:npm": "npm cache clean --force && rm -rf -- node_modules package-lock.json",
    "refresh": "npm run clean:npm && npm install",
    "processes": "lsof -Pnl +M -i -cmd | grep -E 'LISTEN|TCP|UDP'",
    "styles": "gulp build --gulpfile ./tests/styles.ts",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^9.0.4",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/core": "^9.0.4",
    "@angular/forms": "^9.0.4",
    "@angular/localize": "^9.0.4",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/router": "^9.0.4",
    "@junte/serialize-ts": "2.6.6",
    "@ngx-translate/core": "12.1.2",
    "@openfonts/montserrat_cyrillic": "^1.44.1",
    "@openfonts/open-sans_cyrillic": "^1.44.1",
    "@stackblitz/sdk": "^1.5.1",
    "core-js": "3.6.4",
    "date-fns": "2.10.0",
    "faker": "4.1.0",
    "fibers": "^5.0.0",
    "js-beautify": "1.10.3",
    "ng-yandex-metrika": "^3.0.2",
    "ngx-clipboard": "13.0.0",
    "ngx-date-fns": "6.3.0",
    "ngx-highlightjs": "4.1.3",
    "ngx-logger": "^4.1.8",
    "node-html-parser": "1.2.7",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "tslib": "1.11.1",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.4",
    "@angular-devkit/build-ng-packagr": "^0.900.4",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/language-service": "^9.0.4",
    "@types/jasmine": "3.5.7",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.6.2",
    "codelyzer": "5.2.1",
    "gulp": "4.0.0",
    "gulp-debug": "^4.0.0",
    "gulp-iconfont": "10.0.3",
    "gulp-sass": "^4.1.0",
    "gulp-todo": "^7.1.1",
    "gulpclass": "0.2.0",
    "jasmine": "^3.6.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "map-stream": "0.0.7",
    "minimist": "1.2.0",
    "ng-packagr": "9.0.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.3",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "tslint": "6.0.0",
    "typescript": "3.7.2",
    "wait-on": "4.0.1"
  }
}
